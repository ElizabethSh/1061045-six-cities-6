    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "13.0.15",
    "axios-mock-adapter": "1.19.0",
    "babel-loader": "8.2.2",
    "css-loader": "^5.2.7",
    "eslint": "7.18.0",
    "eslint-config-htmlacademy": "2.1.0",
    "eslint-plugin-react": "7.22.0",
    "file-loader": "6.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "redux-mock-store": "1.5.4",
    "style-loader": "2.0.0",
    "webpack": "^5.91.0",
