{
  "name": "six-cities",
  "version": "1.0.0",
  "private": true,
  "description": "Личный проект «Шесть городов» от HTML Academy",
  "scripts": {
    "build": "NODE_OPTIONS='--openssl-legacy-provider' webpack --mode production",
    "start": "NODE_OPTIONS='--openssl-legacy-provider' webpack-dev-server",
    "eslint": "eslint --ext .jsx --ext .js src/",
    "test": "npm run eslint && npm run test.jest",
    "test.jest": "jest"
  },
  "engines": {
    "node": "22.x"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "7.12.10",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.0.15",
    "axios-mock-adapter": "1.19.0",
    "babel-loader": "^8.3.0",
    "css-loader": "^5.2.7",
    "eslint": "7.18.0",
    "eslint-config-htmlacademy": "2.1.0",
    "eslint-plugin-react": "^7.34.1",
    "file-loader": "6.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "redux-mock-store": "1.5.4",
    "style-loader": "^2.0.0",
    "webpack": "^5.94.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "history": "^5.3.0",
    "leaflet": "1.7.1",
    "prop-types": "15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.26.2",
    "redux": "^4.2.1",
    "redux-devtools-extension": "2.13.8",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/src/style-mock.js"
    },
    "setupFilesAfterEnv": [
      "./setup-tests.js"
    ],
    "testEnvironment": "jsdom"
  }
}
